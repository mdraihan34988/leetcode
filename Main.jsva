class Solution {
    public int removeDuplicates(int[] nums) {
        int uniqIndex = 1;
        for(int i=0;i<nums.length-1;i++) {
            if(nums[i] < nums[i+1]) {
                nums[uniqIndex] =  nums[i+1];
                uniqIndex++;
            }
        }

        return uniqIndex;
    }
}